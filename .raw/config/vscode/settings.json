      "filterFileRegex": ".*(?<!CHANGELOG.md)$",
      "decorations": [
        {
          "overviewRulerColor": "#ff5722",
          "backgroundColor": "#ff5722",
          "color": "#ffffff",
          "fontWeight": "bold"
        }
      ]
    },
    // Amber 700 #FFA000
    // DEPRECATED: Deprecated message
    // DEPR: Deprecated message
    // WARN: Warn message
    // HACK: Hack message
